@model IEnumerable<MVCDmeo.Models.Employee>

@using MVCDmeo.Models;

@{
    ViewBag.Title = "Employee List";
}


<div style="font-family:'Times New Roman'">
    <h2>Employee List</h2>


    <ul>

        @foreach(Employee ep in @Model)
        {
            <li>
               @Html.ActionLink(ep.Name, "Details", new { id = ep.EmpId })
            </li>
        }

    </ul>
</div>



